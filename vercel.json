{
    "functions": {
        "api/**/*.php": { "runtime": "vercel-php@0.6.0"
        }
    },
    "routes": [
        {
            "src": "/(.*)",
            "dest": "/api/index.php"
        },
        {
            "src": "/css/style.css",
            "dest": "/assets/css/style-starter.css"
        },
        {
            "src": "/js/script.js",
            "dest": "/assets/js/"
        }
    ]
}